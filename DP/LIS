int longestSubsequence(int n, int a[])
{
   int count=0;
   int max_ele=-32768;
   int dp[n];
   dp[0]=1;
   for(int i=1;i<n;i++){
       int x=a[i];
       dp[i]=1;
       for(int j=0;j<i;j++){
           if(x>a[j]){
               dp[i]=max(dp[i],dp[j]+1);
           }
       }
   }
   for(int i=0;i<n;i++){
       if(dp[i]>=max_ele){
           max_ele=dp[i];
       }
   }
   return max_ele;
}
