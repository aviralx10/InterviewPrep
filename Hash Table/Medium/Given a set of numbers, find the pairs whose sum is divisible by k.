#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    vector<int> A(n);
	    vector<int> B(n,0);
	    for(int i=0;i<n;i++){cin>>A[i];}
	    int x;
	    cin>>x;
	    for(int i=0;i<n;i++){A[i]=A[i]%x;}
	    int y=0;
	    for(int i=0;i<n;i++){if(A[i]==0){y++;}}
	    if(y==n && n%2==0){cout<<"True"<<endl;continue;}
	    map<int,int>s;
	    for(int i=0;i<n;i++){
	        if(s.find(x-A[i])==s.end()){s[A[i]]=i;}
	        else{
	            B[i]=1;
	            int index=s[x-A[i]];
	            B[index]=1;
	        }
	    }
	    int count=0;
	    for(int i=0;i<n;i++){
	        if(B[i]==1){count++;}
	    }
	    if(count==n){cout<<"True"<<endl;}
	    else{cout<<"False"<<endl;}
	}
	return 0;
}
